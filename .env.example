# =============================================================================
# HoneyAgent Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to git (it's in .gitignore)
#
# =============================================================================

# -----------------------------------------------------------------------------
# Auth0 Configuration
# -----------------------------------------------------------------------------
# Your Auth0 tenant domain
AUTH0_DOMAIN=your-tenant.us.auth0.com

# API identifier for your HoneyAgent application
AUTH0_AUDIENCE=https://honeyagent-swarm.api

# Real agent M2M credentials (legitimate agents use these)
AUTH0_REAL_CLIENT_ID=your-real-client-id
AUTH0_REAL_CLIENT_SECRET=your-real-client-secret

# Honeypot agent M2M credentials (fake agents use these)
AUTH0_HONEYPOT_CLIENT_ID=your-honeypot-client-id
AUTH0_HONEYPOT_CLIENT_SECRET=your-honeypot-client-secret

# Auth0 Fine Grained Authorization (FGA) configuration
AUTH0_FGA_STORE_ID=your-fga-store-id
AUTH0_FGA_CLIENT_ID=your-fga-client-id
AUTH0_FGA_CLIENT_SECRET=your-fga-client-secret
AUTH0_FGA_API_URL=https://api.us1.fga.dev

# -----------------------------------------------------------------------------
# AWS Configuration
# -----------------------------------------------------------------------------
# AWS credentials for accessing Bedrock, S3, and other services
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1

# S3 bucket for storing attacker fingerprints and vector data
S3_VECTORS_BUCKET=honeyagent-fingerprints

# Bedrock Knowledge Base ID for threat intelligence queries
HONEYAGENT_KB_ID=your-knowledge-base-id

# -----------------------------------------------------------------------------
# Third-Party Integrations (Optional)
# -----------------------------------------------------------------------------
# Tonic Fabricate - for generating synthetic data
TONIC_API_KEY=your-tonic-api-key
# Alternative env var name (either works)
FABRICATE_API_KEY=your-fabricate-api-key
# Fabricate API URL (optional, defaults to production)
FABRICATE_API_URL=https://fabricate.tonic.ai/api/v1

# TinyFish/AgentQL - for web scraping and browser automation
TINYFISH_API_KEY=your-tinyfish-api-key

# Freepik - for generating visual honeytokens
FREEPIK_API_KEY=your-freepik-api-key
